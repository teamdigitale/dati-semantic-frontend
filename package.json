{
  "name": "digitale",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.1.0",
    "@testing-library/user-event": "^12.1.10",
    "husky": "^7.0.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "stylelint": "13.13.1",
    "stylelint-config-sass-guidelines": "8.0.0",
    "stylelint-config-standard": "22.0.0",
    "web-vitals": "^1.0.1"
  },
  "devDependencies": {
    "react-scripts": "4.0.3",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-jest": "^25.2.2",
    "eslint-plugin-prettier": "^4.0.0",
    "prettier": "^2.4.1"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "lint": "yarn eslint ./src",
    "prepare": "husky install",
    "lint:fix": "eslint --fix './src/**/*.{ts,tsx,js,jsx}'",
    "lint:check": "eslint './src/**/*.{ts,tsx,js,jsx}'",
    "prettier:fix": "prettier --write './src/**/*.{ts,tsx,js,jsx}'",
    "prettier:check": "prettier --check './src/**/*.{ts,tsx,js,jsx}'",
    "postinstall": "sh talisman/talisman-precommit.sh",
    "stylelint:check": "stylelint ./src/**/*.{scss,css}",
    "stylelint:fix": "stylelint ./src/**/*.{scss,css} --fix"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
